@model Eco.Models.KazHydrometWaterPostData
@using Eco.Controllers
@using Microsoft.Extensions.Localization
@inject IStringLocalizer<SharedResources> SharedLocalizer
@{
    ViewData["Title"] = SharedLocalizer["Details"];
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@SharedLocalizer["Details"]</h2>
<div>
    <h4>@SharedLocalizer["KazHydrometWaterPostData"]</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.KazHydrometWaterPost)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.KazHydrometWaterPost.Number)
            @Html.ActionLink(SharedLocalizer["Look"], "Details", "KazHydrometWaterPosts", new { @id = Model.KazHydrometWaterPostId })
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.WaterContaminant)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.WaterContaminant.Name)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Year)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Year)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Month)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Month)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.PollutantConcentrationmgl)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PollutantConcentrationmgl)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">@SharedLocalizer["Edit"]</a> |
    <a asp-action="Index">@SharedLocalizer["BackToList"]</a>
</div>
