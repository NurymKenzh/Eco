    function GoToCompany(id) {
        features = Layer_Companies.getSource().getFeatures();
        found = false;
        for (i = 0; i < features.length && !found; i++) {
            if (features[i].get('Id') === id) {
                found = true;
                map.getView().setCenter(features[i].getGeometry().getCoordinates());
                map.getView().setZoom(18);
            }
        }
    };

