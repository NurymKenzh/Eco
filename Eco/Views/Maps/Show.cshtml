<script>
    $(function () {
        $("#sidepanel").tabs();
    });
    
    var geoserverPort = $('#GeoServerPort').val(),
        geoserverAddress = window.location.hostname;

    // OSM
    var layers = [];
    layers.push(new ol.layer.Tile({
        source: new ol.source.OSM()
    }));
    layers[0].set('name', 'OSM');

    var Source_All = new ol.source.TileWMS({
        url: 'http://' + geoserverAddress + ':' + geoserverPort + '/geoserver/Eco/wms?',
        params: {
            'LAYERS': 'Eco:All',
            'VERSION': '1.1.0',
            'FORMAT': 'image/png',
            'TILED': true
        },
        serverType: 'geoserver'
    });
    var Layer_All = new ol.layer.Tile({
        source: Source_All
    });
    Layer_All.set('name', 'All');

    // 1.1.1 AirPosts
    function AirPostsStyleFunction(feature, resolution) {
        return new ol.style.Style({
            image: new ol.style.Circle({
                radius: 3,
                fill: new ol.style.Fill({ color: [255, 0, 0, 1] }),
                stroke: new ol.style.Stroke({ color: [255, 0, 0, 1], width: 1 })
            })
        });
    };
    var AirPostsLayerJson = $("#AirPostsLayerJson").val();
    var Source_AirPosts = new ol.source.Vector({
        features: (new ol.format.GeoJSON()).readFeatures(AirPostsLayerJson, {
            dataProjection: 'EPSG:4326',
            featureProjection: 'EPSG:3857'
        })
    });
    var Layer_AirPosts = new ol.layer.Vector({
        source: Source_AirPosts,
        style: AirPostsStyleFunction,
        renderBuffer: 200
    });
    Layer_AirPosts.set('name', 'AirPosts');
    Layer_AirPosts.setVisible(false);
    Layer_AirPosts.setOpacity(0.5);
    layers.push(Layer_AirPosts);

    // 1.1.2 KazHydrometAirPosts
    function KazHydrometAirPostsStyleFunction(feature, resolution) {
        return new ol.style.Style({
            image: new ol.style.Circle({
                radius: 3,
                fill: new ol.style.Fill({ color: [0, 255, 0, 1] }),
                stroke: new ol.style.Stroke({ color: [0, 255, 0, 1], width: 1 })
            })
        });
    };
    var KazHydrometAirPostsLayerJson = $("#KazHydrometAirPostsLayerJson").val();
    var Source_KazHydrometAirPosts = new ol.source.Vector({
        features: (new ol.format.GeoJSON()).readFeatures(KazHydrometAirPostsLayerJson, {
            dataProjection: 'EPSG:4326',
            featureProjection: 'EPSG:3857'
        })
    });
    var Layer_KazHydrometAirPosts = new ol.layer.Vector({
        source: Source_KazHydrometAirPosts,
        style: KazHydrometAirPostsStyleFunction,
        renderBuffer: 200
    });
    Layer_KazHydrometAirPosts.set('name', 'KazHydrometAirPosts');
    Layer_KazHydrometAirPosts.setVisible(false);
    Layer_KazHydrometAirPosts.setOpacity(0.5);
    layers.push(Layer_KazHydrometAirPosts);

    // 2.1.1 AnnualMaximumPEV
    var Source_AnnualMaximumPEV = new ol.source.TileWMS({
        url: 'http://' + geoserverAddress + ':' + geoserverPort + '/geoserver/Eco/wms?',
        params: {
            'LAYERS': 'Eco:AnnualMaximumPEV',
            'VERSION': '1.1.0',
            'FORMAT': 'image/png',
            'TILED': true
        },
        serverType: 'geoserver'
    });
    var Layer_AnnualMaximumPEV = new ol.layer.Tile({
        source: Source_AnnualMaximumPEV
    });
    Layer_AnnualMaximumPEV.set('name', 'AnnualMaximumPEV');
    Layer_AnnualMaximumPEV.setVisible(false);
    Layer_AnnualMaximumPEV.setOpacity(0.5);
    layers.push(Layer_AnnualMaximumPEV);

    // 2.1.2 EmissionSources
    function EmissionSourcesStyleFunction(feature, resolution) {
        return new ol.style.Style({
            image: new ol.style.Circle({
                radius: 3,
                fill: new ol.style.Fill({ color: [0, 0, 0, 1] }),
                stroke: new ol.style.Stroke({ color: [0, 0, 0, 1], width: 1 })
            })
        });
    };
    var EmissionSourcesLayerJson = $("#EmissionSourcesLayerJson").val();
    var Source_EmissionSources = new ol.source.Vector({
        features: (new ol.format.GeoJSON()).readFeatures(EmissionSourcesLayerJson, {
            dataProjection: 'EPSG:4326',
            featureProjection: 'EPSG:3857'
